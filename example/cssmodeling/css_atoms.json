{
	"variables":[

		{
			"name":"binary-values",
			"base":"binary",
			"scheme":"binary-values",
			"values":[
				"0","100%"
			],
			"atoms":[
				{
					"name":"binary-width",
					"group":"sizing",
					"example":"<div ... style='height: 100px; background-color:#fff;'></div>",
					"selector":".a-width@var_name_no_base",
					"declaration_value":"width: _@_@var_name;"
				},
				{
					"name":"binary-height",
					"group":"sizing",
					"example":"<div ... style='width: 100px; background-color:#fff;'></div>",
					"selector":".a-height@var_name_no_base",
					"declaration_value":"height: _@_@var_name;"
				},
				{
					"name":"binary-padding",
					"group":"spacing",
					"example":"<div ... style='width: 100px; background-color:#fff;'>Text</div>",
					"selector":".a-padding@var_name_no_base",
					"declaration_value":"padding: _@_@var_name;"
				},
				{
					"name":"binary-margin",
					"group":"spacing",
					"example":"<div ... style='width: 100px; background-color:#fff;'>Text</div>",
					"selector":".a-margin@var_name_no_base",
					"declaration_value":"margin: _@_@var_name;"
				}
			]
		},

		{
			"name":"breakpoint-values",
			"base":"breakpoint",
			"scheme":"breakpoint-values",
			"values":[
				"768px","991px",
                "992px","1199px",
                "1200px","4000px"
			]
		},

		{
			"name":"z-indexes",
			"base":"zindex",
			"shortcut":"<em>@base</em>-[ homePage, projectPage, ... ]",
			"scheme":{"scheme":{"@base":[
				"-homePage",
				"-projectPage",
				"-project",
				"-contentArea","-contentArea-s-project","-contentArea_screen-sm-min",
				"-identityNav",
					"-page_title",

				"-phitheme_copyrightNav"

			]}},
			"values":[
				"4000",
				"2000",
				"6000",
				"1000","5000","2500",
				"4000",
					"200",

				"5000"
			],
			"atoms":[
				{
					"name":"z-index",
					"group":"positioning",
					"example":"...z-layer",
					"selector":".a-@var_name",
					"declaration_value":"z-index : _@_@var_name;"
				}
			]
		},

		{
			"name":"misused",
			"base":"misused",
			"shortcut":"<em>@base</em>-[ on, off ]",
			"scheme":{"scheme":{"@base":[
				"",
				"-off"
			]}},
			"values":[
				"2px solid red",
				"initial"
			],
			"atoms":[
				{
					"name":"misused",
					"group":"misc",
					"example":"...misused",
					"selector":".a-@var_name",
					"declaration_value":"border: _@_@var_name;"
				}
			]
		}


	]
}
